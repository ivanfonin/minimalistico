{
  "name": "Minimalistico",
  "version": "1.0.0",
  "description": "Minimalistico is a WordPress theme development workflow with ES6, Vue.js support. Thanks to Laravel Mix.",
  "main": "",
  "scripts": {
    "browsersync": "npm run dev && gulp &",
    "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run browsersync && cross-env NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "keywords": [],
  "author": "Ivan Fonin",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.18.12",
    "browser-sync-webpack-plugin": "^1.1.4",
    "cross-env": "^3.2.3",
    "gulp": "^3.9.1",
    "laravel-mix": "^1.0.6",
    "webpack": "^2.2.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "jquery": "^3.1.1",
    "vue": "^2.3.4",
    "vue-router": "^2.6.0"
  }
}
